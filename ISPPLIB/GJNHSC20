)Init

  .zvars = '(SCRPOOL)'
  Vget (SCRPOOL) Profile
  if (&SCRPOOL,EQ,'') &SCRPOOL='0'

)ReInit
  .CSRPOS = &ZCMD
  if (.msg Â¬= ' ') .attr(.cursor) = 'Hilite(Reverse)'
)Proc

  ver (&SCRPOOL,NB,range,0,255)
  Vput (SCRPOOL) Profile

)End
